{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "SIGNALING_SERVER": "@signaling-server",
    "REACT_APP_SIGNALING_SERVER": "@webrtc-react-app_signaling_server",
    "REACT_APP_STUN_SERVERS": "@webrtc-react-app_stun_servers",
    "REACT_APP_TURN_SERVERS": "@webrtc-react-app_turn_servers",
    "REACT_APP_TURN_USERNAME": "@webrtc-react-app_turn_username",
    "REACT_APP_TURN_CREDENTIAL": "@webrtc-react-app_turn_credential"
  }
}